# Fastlane Appfile
# Documentation: https://docs.fastlane.tools/advanced/Appfile/

# Apple Developer Portal and App Store Connect credentials
# Can be overridden with environment variables

# Your Apple ID email
apple_id(ENV["FASTLANE_APPLE_ID"] || "your-apple-id@example.com")

# Your iTunes Connect Team ID (if multiple teams)
itc_team_id(ENV["FASTLANE_ITC_TEAM_ID"] || "")

# Your Developer Portal Team ID
team_id(ENV["FASTLANE_TEAM_ID"] || "")

# App identifier
app_identifier("com.deets.app")

# For multiple app identifiers, use:
# for_platform :ios do
#   app_identifier("com.deets.app")
# end
